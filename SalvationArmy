#Challenge G
# Seven Nation Army by The White Stripes

use_bpm 120
use_synth :fm
use_synth_defaults sustain: 0.25
notes =[:e3, :e3, :g3, :e3, :r, :d3, :c3, :b2]
i =0
s =[1.5, 0.5, 0.75, 0.25, 0.5, 0.5, 2 ,2]
live_loop :white_stripes do
  with_fx :autotuner do
    8.times do
      play notes[i]
      sleep s[i]
      i=i+1
      puts(i)
    end
    i=0
  end
end
